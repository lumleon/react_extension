{
  "manifest_version": 3,
  "name": "Order Checker Extension",
  "version": "1.0",
  "description": "A Chrome extension to check order status.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://localhost/siteone/*"],
      "js": ["content.js"]
    }
  ]
}
